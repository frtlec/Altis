                                <li><a href="/Blog/Etkinlikler">Etkinlikler</a></li>

                            </ul>
                        </li>
                        
                        
                   
                        <li id="iletisim"><a href="/iletisim">İletişim</a></li>
                         <li id="Katalog"><a href="~/Content/pdf/Altis-Hizmetiçi-Eğitim-Katalogu.pdf">Katalog</a></li>                       
                    </ul>
                </div>
            </div><!--/.container-->
        </nav><!--/nav-->
		
    </header><!--/header-->

        @if (ViewBag.SideBar != null)
        {
            if (ViewBag.SideBar != "0")
            {
                <div class="container">
                    <div class="col-sm-9">

                        @RenderBody()
                    </div>

                    <div class="col-sm-3">

                        @Html.Partial("_SideBar")
                    </div>

                    </div>

                    }
                    else
                    {
                    @RenderBody()
            }

        }
        else
        {

            <div class="container">
                <br>
                <br>
                <div class="col-sm-9">

                    @RenderBody()
                </div>

                
                <div class="col-sm-3">
                   @Html.Partial("_SideBar")
                </div>

                </div>

                }















                <footer id="footer" class="midnight-blue">
                    <div class="container">
                        <div class="row">
                            <div class="col-sm-6">
                                &copy; 2017 - <a>Altis | İletişim ve Genel Danışmanlık</a>
                            </div>
                            <div class="col-sm-6">
                                <ul class="pull-right">
                                    <li><a href="~Content/http://altis2.mikrooplazma.com/">Ana Sayfa</a></li>
                                    <li><a href="~Content/hakkimizda.html">Hakkımızda</a></li>
                                    <li><a href="~Content/http://altis3.mikrooplazma.com/">Blog</a></li>
                                    <li><a href="~Content/iletisim.html">İletişim</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </footer><!--/#footer-->
</body>
</html>
<script src="~/Content/js/jquery.js"></script>
@Scripts.Render("~/bundles/jqueryui")
